<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:t="clr-namespace:TreeView.Maui.Controls;assembly=TreeView.Maui"
             xmlns:local="clr-namespace:SandboxTreeView.Pages"
             x:Class="SandboxTreeView.Pages.FileSystemPage"
             Title="FileSystemPage">
    <ContentPage.BindingContext>
        <local:FileSystemPageViewModel/>
    </ContentPage.BindingContext>

    <ScrollView>
        <t:TreeView ItemsSource="{Binding Nodes}">

        </t:TreeView>
    </ScrollView>
</ContentPage>
